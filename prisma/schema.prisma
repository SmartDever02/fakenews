generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model logs {
  id                   Int      @id @default(autoincrement())
  uid                  Int      @default(0)
  article              String   @db.Text
  articles_to_review   String[] @db.Text
  original_predictions Float[]  @default([])
  ensemble_predictions Float[]  @default([])
  ensemble_percentage  Float    @default(0)
  predictions          Float[]
  epoch_number         Int      @default(0)
  timestamp            DateTime @default(now())
  token_length         Int      @default(0)
  processing_time      Int      @default(0)
  validator_uid        Int      @default(-1)
}

model skip_logs {
  id           Int      @id @default(autoincrement())
  uid          Int
  epoch_number Int
  timestamp    DateTime @default(now())
}

model metagraph {
  uid       Int   @id
  trust     Float
  stake     Float
  incentive Float
  emission  Float
}

model synth {
  uid           Int     @id @default(autoincrement())
  sigma         Float[]
  spread_factor Float[]
}
