generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model vidaio_hotkey_pool {
  id              Int      @id @default(autoincrement())
  level           Int      @unique
  hotkeyAddresses String[] // List of hotkey addresses (PostgreSQL required for array)
  volume          Int
}
